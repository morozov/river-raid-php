{
    "name": "morozov/river-raid",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "ext-gd": "*"
    },
    "require-dev": {
        "doctrine/coding-standard": "^13.0",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-strict-rules": "^2.0",
        "vimeo/psalm": "^6.10"
    },
    "config": {
        "platform": {
            "php": "8.4.3"
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true
        }
    },
    "autoload": {
        "psr-4": {
            "RiverRaid\\": "src"
        }
    }
}
